<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>шаблон</title>
    <link rel="stylesheet" href="style/css/style.css">
    <!--Plugin ползунок-->
    <link rel="stylesheet"
        href="https://cdnjs.cloudflare.com/ajax/libs/ion-rangeslider/2.3.1/css/ion.rangeSlider.min.css" />

</head>

<body>
    <div class="container">
        <!-- @import header.html -->
        <div class="content indent">
            <div class="banner"></div>

            <div class="wrapped_content">

                <div class="wrapped_products">
                    <h6> Каталог</h6>

                    <div class="Filter">
                        <!-- @import slider.html -->
                        <!-- Фильтр товаров -->
                        <div class="filterProducts">
                            <div class="sortPrice">
                                <p>сортировка по цене:</p> <br>
                                <input id="from" type="text" placeholder="от">
                                <input id="to" type="text" placeholder="до">
                            </div>
                        </div>
                    </div>

                    <div class="products_catalog">
                        <!-- Каталог товаров -->
                    </div>

                </div>
            </div>
        </div>
        <!-- @import footer.html -->
    </div>

    <!--    общие скрипты -->
    <script src="./js/app.js"></script>

    <!--    формирование каталога   -->
    <script src="./js/construct.js"></script>

    <!--    массив с товаром  -->
    <script src="./js/products.js"></script>

    <!--    добавление карточки товара в HTML -->
    <script src="./js/productCards.js"></script>

    <!-- корзина товаров -->
    <script src="./js/basket.js"></script>

    <!-- сортировка по цене -->
    <script src="./js/sortPrice.js"></script>

</body>

</html>